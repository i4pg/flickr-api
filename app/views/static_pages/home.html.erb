<div class="container">
<%= simple_form_for '', method: :get do |f| %>
<%= f.input :user_id %>
<%= f.submit :search %>
<% end %>
<% if @response %>
  <% JSON.parse(@response)["photos"]["photo"].each do |e|%>
    <%= image_tag "https://live.staticflickr.com/#{e["server"]}/#{e["id"]}_#{e["secret"]}.jpg" %>
    <% end %>
  <% end %>
</div>
